{
  "name": "kenokokoro/laravel-basetree-logger",
  "description": "Incoming response/request logger using the database or the file for the laravel-basetree package",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Stefan Brankovikj",
      "email": "stefan.brankovik@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "BaseTree\\": "src/"
    }
  },
  "require": {
    "php": "^7.1",
    "kenokokoro/laravel-basetree": ">1.3",
    "psr/log": "~1.0",
    "ext-json": "*"
  },
  "extra": {
    "laravel": {
      "providers": [
        "BaseTree\\Providers\\BaseTreeLoggerServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
